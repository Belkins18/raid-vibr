{
  "name": "raid-vibr",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "env NODE_ENV=development vite --mode testnet",
    "build": "tsc && vite build",
    "build:dev:testnet": "tsc && env NODE_ENV=development vite build --mode testnet",
    "build:dev:mainnet": "tsc && env NODE_ENV=development vite build --mode mainnet",
    "build:prod:testnet": "tsc && env NODE_ENV=production vite build --mode testnet",
    "build:prod:mainnet": "tsc && env NODE_ENV=production vite build --mode mainnet",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "preview": "vite preview"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not ie <= 99",
      "not android <= 4.4.4",
      "not dead",
      "not op_mini all"
    ]
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "date-fns": "^3.3.1",
    "i18next": "^23.10.1",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-http-backend": "^2.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.1.0",
    "react-router-dom": "^6.22.3",
    "usehooks-ts": "^3.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/node-sass": "^4.11.7",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.37",
    "prettier": "^3.0.3",
    "react-container-query": "^0.13.0",
    "sass": "^1.72.0",
    "typescript": "^5.2.2",
    "vite": "^5.1.6"
  }
}
